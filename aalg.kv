                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "O. Mergesort"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbom
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "O. Quicksort"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
