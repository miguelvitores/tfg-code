                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbl
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "B. Salto"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbs
                                active: False

                    BoxLayout:
                        orientation: 'vertical'
                        rows: 7

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "O. Burbuja"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbob
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
