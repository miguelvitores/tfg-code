                    on_release:
                        self.background_color = (0.4, 0.6078, 0.5647, 1)
                        self.color = (0, 0, 0, 1)
                        root.confirmar(self)
                BoxLayout:
                    size_hint_x: 0.2

        BoxLayout:
            size_hint_x: 0.1


<AbrirProyectoLayout>:
    orientation: 'vertical'
    rows: 2
    label_nombrepr: label_nombrepr
    filechooser: filechooser
    panel_graficas: panel_graficas

    BoxLayout:
        orientation: 'horizontal'
        cols: 5
        size_hint_y: 0.05

        SalirButton:
            text: "Volver"
            size_hint_x: None
            width: 150
            on_release:
                self.background_color = (0.7725, 0.9882, 0.9412, 1)
                root.volver_a_principal(self)

        MediumButton:
            text: "Experimentos"
            size_hint_x: None
            width: 250
            on_release: root.soltar_dpdex(self)

        MediumButton:
            text: "Gr\u00e1ficas interactivas"
            size_hint_x: None
            width: 250
            on_release: root.soltar_dpdgi(self)

        MediumButton:
            text: "Teor\u00eda algoritmos"
            size_hint_x: None
            width: 250
            on_release: root.soltar_dpdta(self)

        Button:
            text: ""
            background_color: (1.0, 0.9412, 0.7922, 1)

    BoxLayout:
        orientation: 'horizontal'
        cols: 2
        size_hint_y: 0.9

        Splitter:
            size_hint_x: 0.25
            sizable_from: 'right'
            min_size: root.width / 8
            max_size: root.width / 2

            BoxLayout:
                orientation: 'vertical'
                rows: 2

                MediumLabel:
                    id: label_nombrepr
                    font_size: 20
                    size_hint_y: 0.046
                    color: (1, 1, 1, 1)

                    canvas.before:
                        Color:
                            rgba: (0.0039, 0.4588, 0.3647, 1)
                        Rectangle:
                            size: self.size
                            pos: self.pos

                FileChooserListView:
                    id: filechooser
                    size_hint_y: 0.954
                    filters: ["*.png"]
                    on_selection: root.seleccionar_archivo(self.selection)

        TabbedPanel:
            id: panel_graficas
            size_hint_x: 0.75
            background_color: (0.7725, 0.9882, 0.9412, 1)
            do_default_tab: False
            tab_width: root.width / 8

            canvas.before:
                Color:
                    rgba: (0.4, 0.6078, 0.5647, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos


