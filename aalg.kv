                        MiniThinLabel:
                            text: "Seleccionar\ntodos"
                            size_hint_x: 0.4
                            halign: 'right'

                        Switch:
                            id: switch_sel_ao
                            active: False
                            on_active: root.seleccionar_todos_ao(self)

                    BoxLayout:
                        orientation: 'vertical'
                        rows: 6

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "B. Binaria"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbb
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "B. Exponencial"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbe
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "B. Fibonacci"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbf
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
                                text: "B. Interpolaci\u00f3n"

                            CheckBox:
                                color: (0.0039, 0.4588, 0.3647, 1)
                                id: cbbi
                                active: False

                        BoxLayout:
                            orientation: 'horizontal'
                            cols: 2

                            MicroLabel:
                                text_size: self.size
                                valign: 'center'
                                halign: 'right'
