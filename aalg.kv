                        text: "Algoritmos de b\u00fasqueda"
                        size_hint_y: 0.1

                    MiniLabel:
                        text: "Algoritmos de ordenaci\u00f3n"
                        size_hint_y: 0.1

                    BoxLayout:
                        orientation: 'horizontal'
                        cols: 2
                        size_hint_y: 0.05

